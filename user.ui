    color: #2C2C2C;
    font-size: 14px;
    font-weight: 600;
    padding: 0px 14px;
    min-width: 80px;
    min-height: 30px;
    padding-right: 30px; /* space for arrows */
}

/* Up/Down buttons */
QSpinBox::up-button,
QSpinBox::down-button {
    background-color: #E0E0E0;
    border: 1px solid #2A4E7C;
    width: 20px;
    subcontrol-origin: border;
}
QSpinBox::down-button{
    border-top: none;
}

/* Hover buttons */
QSpinBox::up-button:hover,
QSpinBox::down-button:hover {
    background-color: #93b7e1;
}

/* Pressed buttons */
QSpinBox::up-button:pressed,
QSpinBox::down-button:pressed {
    background-color: #DADADA;
}

/* Arrow symbols â€” force them to center */
QSpinBox::up-arrow,
QSpinBox::down-arrow {
    width: 10px;
    height: 10px;
    margin: 0px;   /* prevent shifting */
    padding: 0px;  /* keep centered */
}

QSpinBox::up-arrow {
    image: url(:/icon/arrow-up-1.png);
}

QSpinBox::down-arrow {
    image: url(:/icon/expand-arrow.png);
}
