    padding: 10px 15px; /* Padding inside the field */
    box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.4); /* Subtle shadow for a raised effect */
}

/* Placeholder Text Styling */
QLineEdit::placeholder {
    color: #888888; /* Light grey placeholder text */
    font-style: italic; /* Italicized placeholder text */
}

/* Focused LineEdit Styling */
QLineEdit:focus {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 #2e2e2e, stop:1 #3e3e3e); /* Slightly brighter gradient */
    border: 2px solid #d4af37; /* Gold border for focus */
    box-shadow: 0px 6px 10px rgba(212, 175, 55, 0.5); /* Gold glow when focused */
}

/* Disabled LineEdit Styling */
QLineEdit:disabled {
    background-color: #555555; /* Grey background for disabled state */
    color: #bbbbbb; /* Faded text */
    border: 2px solid #666666; /* Subtle grey border */
}

/* Hover Effect */
QLineEdit:hover {
    border: 2px solid #f7e09d; /* Light gold border on hover */
    box-shadow: 0px 5px 8px rgba(247, 224, 157, 0.4); /* Light gold glow on hover */
}
