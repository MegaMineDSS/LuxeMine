              <property name="styleSheet">
               <string notr="true">/* General QCheckBox Styling */
QCheckBox {
    font-size: 14px; /* Font size for label text */
    color: white; /* White text color */
    font-weight: bold; /* Bold label */
    spacing: 8px; /* Space between checkbox and text */
}

/* Checkbox Indicator */
QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border-radius: 3px;
    border: 2px solid #d4af37; /* Gold border */
    background-color: #222222; /* Dark background */
}

/* Checked State */
QCheckBox::indicator:checked {
    background-color: #d4af37; /* Gold color when checked */
    border: 2px solid #ffffff; /* White border */
}

/* Hover Effect */
QCheckBox::indicator:hover {
    border: 2px solid #f7e09d; /* Light gold border on hover */
}

/* Disabled Checkbox */
QCheckBox::indicator:disabled {
    background-color: #555555; /* Greyed-out background */
    border: 2px solid #666666; /* Greyed-out border */
}

QCheckBox:disabled {
    color: #888888; /* Greyed-out text */
}
